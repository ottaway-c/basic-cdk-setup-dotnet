    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.34.2",
    "constructs": "^10.0.0",
    "dotenv": "^16.0.1",
    "env-var": "^7.1.1",
    "source-map-support": "^0.5.21"
  },
  "prettier": {
    "printWidth": 120
